{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**/*"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/bot1",
            "dest": "/src/index.js"
        },
        {
            "src": "/bot2",
            "dest": "/src/index.js"
        }
    ]
}